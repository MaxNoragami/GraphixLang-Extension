{
  "scopeName": "source.graphixlang",
  "patterns": [
    {
      "name": "keyword.graphixlang",
      "match": "\\b(if|else|for|while|function|return|let|const|var|class|import|export)\\b"
    },
    {
      "name": "variable.other.graphixlang",
      "match": "\\b([a-zA-Z_][a-zA-Z0-9_]*)\\b"
    },
    {
      "name": "string.quoted.double.graphixlang",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "name": "constant.character.escape.graphixlang",
          "match": "\\\\."
        }
      ]
    },
    {
      "name": "comment.line.double-slash.graphixlang",
      "match": "//.*$"
    },
    {
      "name": "comment.block.graphixlang",
      "begin": "/\\*",
      "end": "\\*/"
    }
  ],
  "repository": {},
  "scopeName": "source.graphixlang"
}