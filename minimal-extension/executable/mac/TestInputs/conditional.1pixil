{
    BATCH #myBatch = "../TestImgs/Input";
    FOREACH IMG $image IN #myBatch EXPORT TO "../TestImgs/Output"{
        INT $width = 0;
        $width = METADATA $image FWIDTH;
        
        IF $width > 1000 {
            SET $image SEPIA;
        }
        ELIF $width >= 500 {
            SET $image BW;
        }
        ELIF $width >= 300 {
            SET $image NEGATIVE;
        }
        ELSE {
            SET $image SHARPEN;
        }
    }
}